        "descriptionForeground": "#9898b0",
        "icon.foreground": "#9898b0"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#4a4a68",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#69f0ae"
            }
        },
        {
            "name": "Template Strings",
            "scope": [
                "string.template",
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#69f0ae"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#ffd580"
            }
        },
        {
            "name": "Booleans & Null",
            "scope": [
                "constant.language"
            ],
            "settings": {
                "foreground": "#ff8a80"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#b39ddb"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#48CFCB"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#82b1ff"
            }
        },
        {
            "name": "Function Parameters",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#ffcc80",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Classes & Types",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#48CFCB"
            }
        },
        {
            "name": "Interfaces & Type Params",
            "scope": [
                "entity.name.type.interface",
                "entity.name.type.parameter"
            ],
            "settings": {
                "foreground": "#48CFCB",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#d8d8e8"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#ffd580"
            }
        },
        {
            "name": "Properties",
            "scope": [
                "variable.other.property",
                "support.variable.property"
            ],
            "settings": {
                "foreground": "#b0bec5"
            }
        },
        {
            "name": "Tags (HTML/JSX)",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#ff8a80"
            }
        },
        {
            "name": "Tag Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ffcc80",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "entity.name.tag.css",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.id.css"
            ],
            "settings": {
                "foreground": "#48CFCB"
            }
        },
        {
            "name": "CSS Properties",
            "scope": [
                "support.type.property-name.css"
            ],
            "settings": {
                "foreground": "#82b1ff"
            }
        },
        {
            "name": "CSS Values",
            "scope": [
                "support.constant.property-value.css",
                "meta.property-value.css"
            ],
            "settings": {
                "foreground": "#ffd580"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "meta.decorator",
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "#ffcc80"
            }
        },
        {
            "name": "Regex",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#ff8a80"
            }
        },
        {
            "name": "Escape Characters",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#48CFCB"
            }
        },
        {
            "name": "Punctuation â€” Brackets",
            "scope": [
                "punctuation.definition.block",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#9898b0"
            }
        },
        {
            "name": "Markdown Heading",
            "scope": [
                "markup.heading",
                "entity.name.section.markdown"
            ],
            "settings": {
                "foreground": "#6C63FF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#ffd580",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markdown Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#b39ddb",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markdown Link",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#48CFCB"
            }
        },
        {
            "name": "Markdown Code",
            "scope": [
                "markup.inline.raw",
                "markup.fenced_code.block"
            ],
            "settings": {
                "foreground": "#69f0ae"
            }
        },
        {
            "name": "JSON Key",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#82b1ff"
            }
        },
        {
            "name": "YAML Key",
            "scope": [
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#48CFCB"
            }
        },
        {
            "name": "Python Self/Cls",
            "scope": [
                "variable.language.special.self",
                "variable.parameter.function.language.special.self"
            ],
            "settings": {
                "foreground": "#ff8a80",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Rust Lifetime",
            "scope": [
                "storage.modifier.lifetime.rust",
                "entity.name.type.lifetime.rust"
            ],
            "settings": {
                "foreground": "#ffcc80",
                "fontStyle": "italic"
            }
        }
    ],
    "semanticTokenColors": {
        "namespace": "#9898b0",
        "type": "#48CFCB",
        "class": "#48CFCB",
        "enum": "#48CFCB",
        "interface": {
            "foreground": "#48CFCB",
            "italic": true
        },
        "struct": "#48CFCB",
        "typeParameter": {
            "foreground": "#ffcc80",
            "italic": true
        },
        "parameter": {
            "foreground": "#ffcc80",
            "italic": true
        },
        "variable": "#d8d8e8",
        "property": "#b0bec5",
        "enumMember": "#ffd580",
        "decorator": "#ffcc80",
        "event": "#82b1ff",
        "function": "#82b1ff",
        "method": "#82b1ff",
        "macro": "#b39ddb",
        "label": "#ffd580",
        "comment": {
            "foreground": "#4a4a68",
            "italic": true
        },
        "string": "#69f0ae",
        "keyword": "#b39ddb",
        "number": "#ffd580",
        "regexp": "#ff8a80",
        "operator": "#48CFCB"
    }
}