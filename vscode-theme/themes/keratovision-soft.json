        "gitDecoration.deletedResourceForeground": "#f5a0a0",
        "gitDecoration.untrackedResourceForeground": "#8adbb4",
        "gitDecoration.ignoredResourceForeground": "#505068",
        "editorError.foreground": "#f5a0a0",
        "editorWarning.foreground": "#ffe0a0",
        "editorInfo.foreground": "#a0c4ff",
        "focusBorder": "#7c74e866",
        "foreground": "#cdd6f4",
        "widget.shadow": "#00000033",
        "selection.background": "#7c74e82a",
        "descriptionForeground": "#a0a0b8",
        "icon.foreground": "#a0a0b8"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#505068",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string",
                "string.quoted"
            ],
            "settings": {
                "foreground": "#8adbb4"
            }
        },
        {
            "name": "Template Strings",
            "scope": [
                "string.template"
            ],
            "settings": {
                "foreground": "#8adbb4"
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#ffe0a0"
            }
        },
        {
            "name": "Booleans",
            "scope": [
                "constant.language"
            ],
            "settings": {
                "foreground": "#f5a0a0"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#c8b8e8"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "#70d8d4"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.function-call",
                "support.function"
            ],
            "settings": {
                "foreground": "#a0c4ff"
            }
        },
        {
            "name": "Params",
            "scope": [
                "variable.parameter"
            ],
            "settings": {
                "foreground": "#ffd8a0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Types",
            "scope": [
                "entity.name.type",
                "entity.name.class",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#70d8d4"
            }
        },
        {
            "name": "Interfaces",
            "scope": [
                "entity.name.type.interface"
            ],
            "settings": {
                "foreground": "#70d8d4",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#cdd6f4"
            }
        },
        {
            "name": "Constants",
            "scope": [
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#ffe0a0"
            }
        },
        {
            "name": "Properties",
            "scope": [
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#bbc8d8"
            }
        },
        {
            "name": "HTML Tags",
            "scope": [
                "entity.name.tag"
            ],
            "settings": {
                "foreground": "#f5a0a0"
            }
        },
        {
            "name": "Attributes",
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#ffd8a0",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Selectors",
            "scope": [
                "entity.name.tag.css",
                "entity.other.attribute-name.class.css"
            ],
            "settings": {
                "foreground": "#70d8d4"
            }
        },
        {
            "name": "CSS Props",
            "scope": [
                "support.type.property-name.css"
            ],
            "settings": {
                "foreground": "#a0c4ff"
            }
        },
        {
            "name": "Regex",
            "scope": [
                "string.regexp"
            ],
            "settings": {
                "foreground": "#f5a0a0"
            }
        },
        {
            "name": "Escape",
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#70d8d4"
            }
        },
        {
            "name": "Brackets",
            "scope": [
                "punctuation.definition.block",
                "meta.brace"
            ],
            "settings": {
                "foreground": "#a0a0b8"
            }
        },
        {
            "name": "MD Heading",
            "scope": [
                "markup.heading"
            ],
            "settings": {
                "foreground": "#7c74e8",
                "fontStyle": "bold"
            }
        },
        {
            "name": "MD Bold",
            "scope": [
                "markup.bold"
            ],
            "settings": {
                "foreground": "#ffe0a0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "MD Italic",
            "scope": [
                "markup.italic"
            ],
            "settings": {
                "foreground": "#c8b8e8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "MD Link",
            "scope": [
                "markup.underline.link"
            ],
            "settings": {
                "foreground": "#70d8d4"
            }
        },
        {
            "name": "MD Code",
            "scope": [
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "#8adbb4"
            }
        },
        {
            "name": "JSON Key",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#a0c4ff"
            }
        },
        {
            "name": "YAML Key",
            "scope": [
                "entity.name.tag.yaml"
            ],
            "settings": {
                "foreground": "#70d8d4"
            }
        }
    ],
    "semanticTokenColors": {
        "namespace": "#a0a0b8",
        "type": "#70d8d4",
        "class": "#70d8d4",
        "interface": {
            "foreground": "#70d8d4",
            "italic": true
        },
        "parameter": {
            "foreground": "#ffd8a0",
            "italic": true
        },
        "variable": "#cdd6f4",
        "property": "#bbc8d8",
        "function": "#a0c4ff",
        "method": "#a0c4ff",
        "keyword": "#c8b8e8",
        "string": "#8adbb4",
        "number": "#ffe0a0",
        "operator": "#70d8d4",
        "comment": {
            "foreground": "#505068",
            "italic": true
        }
    }
}