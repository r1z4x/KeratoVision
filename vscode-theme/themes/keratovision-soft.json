        "terminal.ansiBrightYellow": "#ffe0a0",
        "terminal.ansiBrightBlue": "#a0c4ff",
        "terminal.ansiBrightMagenta": "#c8b8e8",
        "terminal.ansiBrightCyan": "#70d8d4",
        "terminal.ansiBrightWhite": "#e8e8f4",
        "input.background": "#1e1e32",
        "input.foreground": "#cdd6f4",
        "input.border": "#2a2a44",
