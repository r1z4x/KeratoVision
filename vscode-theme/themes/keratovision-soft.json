        "tab.hoverBackground": "#222236",
        "editorGroupHeader.tabsBackground": "#14141e",
        "editorGroupHeader.tabsBorder": "#1e1e32",
        "panel.background": "#14141e",
        "panel.border": "#1e1e32",
        "panelTitle.activeBorder": "#7c74e8",
        "panelTitle.activeForeground": "#cdd6f4",
        "panelTitle.inactiveForeground": "#707088",
        "terminal.background": "#14141e",
        "terminal.foreground": "#cdd6f4",
        "terminal.ansiBlack": "#1e1e32",
        "terminal.ansiRed": "#f5a0a0",
        "terminal.ansiGreen": "#8adbb4",
        "terminal.ansiYellow": "#ffe0a0",
