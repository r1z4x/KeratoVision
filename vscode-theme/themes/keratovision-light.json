        "editor.inactiveSelectionBackground": "#6C63FF11",
        "editor.wordHighlightBackground": "#6C63FF18",
        "editor.findMatchBackground": "#48CFCB33",
        "editor.findMatchHighlightBackground": "#48CFCB1a",
        "editorCursor.foreground": "#6C63FF",
        "editorWhitespace.foreground": "#d8d5cc40",
